let totalE = 0;
for(let col = 0; col < data.systems[0].dysonPoints.length; col++) {
	for(let row = 0; row < data.systems[0].dysonPoints[0].length; row++) {
		if(data.systems[0].dysonPoints[col][row] === 2) {
			totalE++;
		}
	}
}
console.log(totalE);

 let totalLeft = 0;
for(let col = 0; col < data.systems[0].dysonPoints.length; col++) {
	for(let row = 0; row < data.systems[0].dysonPoints[0].length; row++) {
		if(data.systems[0].dysonPoints[col][row] === 1) {
			totalLeft++;
		}
	}
}
console.log(totalLeft);


